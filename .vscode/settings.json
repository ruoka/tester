{
  "files.associations": {
    "*.h++": "cpp",
    "*.c++": "cpp",
    "*.c++m": "cpp"
  }
}

{
    "files.associations": {
        "*.hpp": "cpp",
        "*.cpp": "cpp",
        "*.c++": "cpp",
        "*.c++m": "cpp"
    },
    "C_Cpp.default.compilerPath": "/usr/lib/llvm-19/bin/clang++",
    "C_Cpp.default.compilerArgs": [
        "-std=c++23",
        "-stdlib=libc++",
        "-fexperimental-library",
        "-pthread",
        "-fPIC",
        "-I/usr/include/c++/v1",
        "-fprebuilt-module-path=${workspaceFolder}/build/pcm"
    ],
    "C_Cpp.default.cppStandard": "c++23",
    "C_Cpp.default.intelliSenseMode": "clang-x64",
    "C_Cpp.default.includePaths": [
        "${workspaceFolder}/tester",
        "/usr/include/c++/v1"
    ],
    "C_Cpp.errorSquiggles": "enabled",
    "C_Cpp.configurationProvider": "ms-vscode.makefile-tools",
    "makefile.launchConfigurations": [
        {
            "cwd": "${workspaceFolder}",
            "binaryPath": "${workspaceFolder}/build/bin/test_runner",
            "binaryArgs": ["--help"]
        }
    ],
    "terminal.integrated.gpuAcceleration": "off",
    "terminal.integrated.defaultProfile.linux": "bash",
    "files.exclude": {
        "**/build/**": true,
        "**/obj/**": true,
        "**/pcm/**": true,
        "**/*.o": true,
        "**/*.a": true
    },
    "search.exclude": {
        "**/build/**": true,
        "**/obj/**": true,
        "**/pcm/**": true
    },
    "C_Cpp.default.defines": [],
    "C_Cpp.default.browse.limitSymbolsToIncludedHeaders": false
}

